/*---< REXX >---*/
ARG LINEA
FICH='';MIEMB=''
DO I=1 TO LENGTH(LINEA)
   LT=SUBSTR(LINEA,I,1)
   IF LETRA(LT,'0') THEN FICH=FICH||LT
   ELSE IF LT='.' THEN FICH=FICH||LT
        ELSE IF LT='(' THEN CALL MIEMBRO
             ELSE LEAVE
END
ADDRESS ISPEXEC
'VPUT (FICH,MIEMB)'
RETURN 0
MIEMBRO:
   DO UNTIL I=LENGTH(LINEA)
      I=I+1
      LT=SUBSTR(LINEA,I,1)
      IF LETRA(LT,'1') THEN MIEMB=MIEMB||LT
      ELSE LEAVE
   END
RETURN
LETRA:
  ARG LT,AST
  IF DATATYPE(LT,'A') | LT='-' | LT='+' THEN RETURN 1
  ELSE IF AST THEN RETURN LT='@'|LT='%'|LT='$'|LT='*'
       ELSE RETURN LT='@'|LT='%'|LT='$'
