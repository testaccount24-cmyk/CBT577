/*REXX*/
SAY 'WHICH DDNAME DO YOU WANT ?'
SAY '   INTRO GIVES ALL   '
PULL DDNAME
IF DDNAME='' THEN DDNAME='TODAS'
X=OUTTRAP("LIN.")
"LISTA ST"
SAY ' DDNAMES       DSNAMES                                    '
SAY '----------    --------------------------------------------'
IF TRANSLATE(DDNAME)='TODAS' THEN DO
  DO I=2 TO LIN.0
     IF SUBSTR(LIN.I,1,2)='  ' &  SUBSTR(LIN.I,3,1)¬=' ' THEN DO
        DDNAME=WORD(LIN.I,1)
        CALL PROGTOT
     END
  END
END
ELSE DO
   DO I=2 TO LIN.0
      IF WORD(LIN.I,1)=DDNAME THEN LEAVE
   END
   CALL PROGTOT
END
EXIT

PROGTOT:
IF I>LIN.0 THEN EXIT
K=I-1 ; NUM=1
SAY ' 'LEFT(DDNAME,13)RIGHT(NUM,2)'.- 'STRIP(LIN.K)
DO K=I+1 TO LIN.0 BY 2
    J=K+1
    PARSE VAR LIN.J UNO 12 .
    IF STRIP(UNO)¬='' THEN LEAVE
    NUM=NUM+1
    SAY '              'RIGHT(NUM,2)'.- 'STRIP(LIN.K)
END
SAY ' ' ; SAY ' '
RETURN
